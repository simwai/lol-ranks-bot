{
  "name": "lol-ranks-bot",
  "version": "1.0.0",
  "description": "A Discord bot to assign roles based on League of Legends Rank",
  "keywords": [
    "Discord",
    "Bot",
    "League of Legends",
    "LoL"
  ],
  "license": "CC BY-NC-SA 4.0",
  "author": "simwai",
  "main": "bot.js",
  "scripts": {
    "always-debug": "tsx watch --ignore ./tests --inspect --trace-warnings ./src/bot.ts",
    "debug": "tsx --inspect --trace-warnings ./src/bot.ts",
    "lint": "eslint --ext .ts,.json,.eslintrc --fix .",
    "format": "prettier-eslint --write \"src/**/*.ts\"",
    "start": "tsx src/bot.ts",
    "test": "npx ava",
    "stop-debug": "taskkill /F /IM node.exe"
  },
  "engines": {
    "node": ">=20"
  },
  "type": "module",
  "dependencies": {
    "@discordjs/rest": "^0.4.1",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "bottleneck": "^2.19.5",
    "cron": "^3.1.7",
    "discord-api-types": "^0.32.1",
    "discord.js": "13.17.1",
    "got": "^14.2.1",
    "i18n": "^0.15.1",
    "lowdb": "^1.0.0",
    "luxon": "^3.4.4",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@types/i18n": "^0.13.10",
    "@types/lowdb": "^1.0.15",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.12.7",
    "ava": "^6.1.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-json-format": "^2.0.1",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^8.0.3",
    "nock": "^13.5.4",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "prettier-eslint": "^16.3.0",
    "prettier-eslint-cli": "^8.0.1",
    "sinon": "^17.0.1",
    "tsx": "^4.7.2",
    "typescript": "^5.4.5"
  }
}
